    "@types/jest": "28.1.6",
    "@types/node": "18.6.1",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.3",
    "nx": "14.4.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
