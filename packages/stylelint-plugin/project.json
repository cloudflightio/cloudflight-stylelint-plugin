{
  "root": "packages/stylelint-plugin",
  "sourceRoot": "packages/stylelint-plugin/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/stylelint-plugin",
        "main": "packages/stylelint-plugin/src/index.js",
        "tsConfig": "packages/stylelint-plugin/tsconfig.lib.json",
        "assets": ["packages/stylelint-plugin/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/stylelint-plugin/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/stylelint-plugin"],
      "options": {
        "jestConfig": "packages/stylelint-plugin/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
